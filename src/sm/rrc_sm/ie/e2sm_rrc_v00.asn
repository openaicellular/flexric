-- ---------------------------------------------------
-- RAN Function definition OCTET STRING contents
-- ---------------------------------------------------

-- Not defined in this E2SM




-- ---------------------------------------------------
-- commmon IEs
-- ---------------------------------------------------


RRCStats-Header ::= INTEGER

RRCStats-Message ::= SEQUENCE
{
    ueStats           SEQUENCE OF RRCUeStats,
    tstamp            INTEGER,
    ...
}


RRCUeStats ::= SEQUENCE
{
    rnti                    INTEGER(0..4294967295),
    rbid                    INTEGER(0..255),
    drbActive               SEQUENCE OF INTEGER(0..255),
    statusRrc               INTEGER(0..255),
    setupPduSession         INTEGER(0..255),
    nbOfPdusessions         INTEGER(0..255),
    ueRrcInactivityTimer    INTEGER(0..255),
    ...
}

RRCStats-TriggerNature ::= ENUMERATED{
	oneMs,
	twoMs,
	fiveMs,
	...
}

RANparameter-Item ::= SEQUENCE {
	ranParameter-ID			RANparameter-ID,
	ranParameter-Name		RANparameter-Name,
	ranParameter-Test		RANparameter-Test,
	ranParameter-Value		RANparameter-Value,
	...
}


RANparameter-ID ::= INTEGER (0..maxofRANParameters)

RANparameter-Name ::= OCTET STRING

RANparameter-Test ::= ENUMERATED{
	equal,
	greaterthan,
	lessthan,
	contains,
	present,
	...
}

RANparameter-Value ::= OCTET STRING


RRCStats-ControlOutcome ::= ENUMERATED{
	rrcCtrlOutcomeOk,
	rrcCtrlOutcomeEnd,
	...
}

END

-- ASN1 STOP