E2SM-RRCStats-ActionDefinition ::= CHOICE{
	actionDefinition-Format1		E2SM-RRCStats-ActionDefinition-Format1,
	...
}

-- E2SM-RRCStats-actionDefinition IE used for Action Format 1
E2SM-RRCStats-ActionDefinition-Format1 ::= SEQUENCE{
	ranParameter-List					SEQUENCE (SIZE(1..maxofRANParameters)) OF RANparameter-Item OPTIONAL,
	...
}


-- ---------------------------------------------------
-- Indication Header OCTET STRING contents
